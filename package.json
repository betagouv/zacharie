{
  "name": "zacharie",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "yarn prisma-deploy && react-dsfr update-icons && node ./server.js",
    "dev-test-react-dsfr": "react-dsfr update-icons && node ./server.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "prisma-setup": "prisma generate && prisma migrate deploy",
    "prisma-create-migration": "NODE_ENV=development POSTGRESQL_ADDON_URI=postgresql://postgres@localhost/zacharie-temp npx prisma migrate dev --name",
    "prisma-deploy": "NODE_ENV=production npx prisma migrate deploy",
    "prisma-reset-db": "rm -rf prisma/migrations && npx prisma migrate reset --force && npm run prisma-setup",
    "start": "npx prisma migrate deploy && react-dsfr update-icons && cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.13.2",
    "@prisma/client": "^5.19.1",
    "@remix-run/express": "^2.11.2",
    "@remix-run/node": "^2.11.2",
    "@remix-run/react": "^2.11.2",
    "@sentry/remix": "^8.28.0",
    "@sentry/vite-plugin": "^2.22.3",
    "@uidotdev/usehooks": "^2.4.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "express": "^4.19.2",
    "isbot": "^4.1.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.6.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.11.2",
    "@types/bcryptjs": "^2",
    "@types/compression": "^1.7.5",
    "@types/crypto-js": "^4",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vite-pwa/remix": "^0.1.3",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "prisma": "^5.19.1",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-plugin-pwa": "^0.20.5",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@4.4.1",
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "semi": true,
    "printWidth": 120
  }
}
