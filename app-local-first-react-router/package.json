    "@react-hookz/web": "^25.1.1",
    "@sentry/react": "^9.22.0",
    "@uidotdev/usehooks": "^2.4.1",
    "compression": "^1.7.5",
    "dayjs": "^1.11.13",
    "express": "^4.21.1",
    "idb-keyval": "^6.2.2",
    "p-queue": "^8.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-router": "^7.6.0",
    "react-select": "^5.10.1",
