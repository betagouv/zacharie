    "workbox-precaching": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "printWidth": 110,
    "tabWidth": 2,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
